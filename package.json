    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.17.0",
    "http-errors": "^1.7.3",
    "mongoose": "^5.7.11",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "postcss-middleware": "^1.1.4",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
